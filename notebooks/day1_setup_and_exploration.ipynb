"""
ğŸ¯ TASK: Assess data quality across all datasets
ğŸ’¡ HINT: Check for missing values, duplicates, data ranges
ğŸ“š CONCEPTS: Data profiling, quality metrics, anomaly detection
"""

def assess_data_quality(df, dataset_name):
    """
    Perform basic data quality assessment on a DataFrame
    
    Args:
        df: Spark DataFrame to assess
        dataset_name: Name of the dataset for reporting
    """
    print(f"\nğŸ“‹ Data Quality Assessment: {dataset_name}")
    print("-" * 50)
    
    # TODO: Basic statistics
    total_rows = df.count()
    total_cols = len(df.columns)
    print(f"   ğŸ“Š Dimensions: {total_rows:,} rows Ã— {total_cols} columns")
    
    # TODO: Check for missing values
    print(f"   ğŸ” Missing Values:")
    for col in df.columns:
        missing_count = df.filter(F.col(col).isNull()).count()
        missing_pct = (missing_count / total_rows) * 100
        if missing_count > 0:
            print(f"      {col}: {missing_count:,} ({missing_pct:.2f}%)")
    
    # TODO: Check for duplicate records
    duplicate_count = total_rows - df.dropDuplicates().count()
    if duplicate_count > 0:
        print(f"   ğŸ”„ Duplicate Records: {duplicate_count:,}")
    else:
        print(f"   âœ… No duplicate records found")
    
    # TODO: Numeric column statistics
    numeric_cols = [field.name for field in df.schema.fields 
                   if field.dataType in [IntegerType(), DoubleType(), FloatType(), LongType()]]
    
    if numeric_cols:
        print(f"   ğŸ“ˆ Numeric Columns Summary:")
        # Show basic statistics for numeric columns
        df.select(numeric_cols).describe().show()

